set encoding=utf-8

set incsearch

set number
set relativenumber
set syntax=enable

set shiftwidth=4
set tabstop=4
set smarttab
set softtabstop=0
set expandtab
set autoindent

call plug#begin()
Plug 'vim-airline/vim-airline'
Plug 'neovim/nvim-lspconfig'
Plug 'nvim-treesitter/nvim-treesitter'
Plug 'nvim-lua/plenary.nvim'
Plug 'nvim-telescope/telescope.nvim'
call plug#end()
